{
    "name": "erikaraujo/php-enhanced-enums",
    "description": "Enhanced enum methods for PHP enums",
    "keywords": [
        "enum",
        "php",
        "enhanced"
    ],
    "require": {
        "php": "^8.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ErikAraujo\\PhpEnhancedEnums\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ErikAraujo\\PhpEnhancedEnums\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Erik Araujo",
            "email": "contact@erikaraujo.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^11.1",
        "laravel/pint": "^1.15",
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "format": "./vendor/bin/pint",
        "test": "./vendor/bin/phpunit",
        "analyse": "./vendor/bin/phpstan analyse"
    }
}
